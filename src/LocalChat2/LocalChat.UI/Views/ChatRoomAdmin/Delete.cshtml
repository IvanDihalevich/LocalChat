﻿@using LocalChat.Core.Entities
@model ChatRoom

@{
    var types = ViewBag.ChatRoom as List<SelectListItem>;
}

<h1>Видалення проєкту: @Model.Name</h1>


<form asp-action="Delete" asp-controller="ChatRoomAdmin">
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.Id)
    <input type="text" asp-for="Id" value="@Model.Id" />
    <button class="btn btn-primary" type="submit">Видалити</button>
</form>